{
  "name": "cibincasso/barcode",
  "type": "symfony-bundle",
  "description": "Symfony2 Barcode Generator Bundle with Twig function extension",
  "keywords": [
    "Symfony2",
    "barcode",
    "Symfony",
    "Bundle",
    "Twig",
    "generator",
    "qrcode",
    "qr code"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Cibincasso",
      "email": "development@cibincasso.nl"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/framework-bundle": ">=2.3",
    "symfony/twig-bundle": ">=2.3",
    "symfony/options-resolver": ">=2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.0"
  },
  "autoload": {
    "psr-4": { "Cibincasso\\BarcodeBundle\\": "" }
  },
  "minimum-stability": "stable"
}
